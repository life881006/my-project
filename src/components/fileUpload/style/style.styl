.el-fileUpload-list
	width:100%
	display:-webkit-flex
	display:flex
	flex-wrap:wrap
	align-items:flex-start
	
	
	.annexItem
		width:260px
		padding:30px 20px 20px 20px
		position:relative
		border:1px solid #dcdfe6
		border-radius:4px
		margin:10px 10px 10px 0px
		overflow:hidden
		z-index:50
		display:flex
		flex-direction: column
			
		
		.serialNumberArea
			width:60px
			height:60px
			transform:rotate(45deg)
			position:absolute
			top:-30px;
			left:-30px;
			text-align:right;
			background-color:#409eff
			overflow:hidden
			
			.serialNumer
				width:30px;
				height:30px;
				text-align:center
				position:absolute
				right:0
				bottom:20px
				transform: rotate(-45deg)
				display:block
				color:#fff;
				z-index:200;
								
		.delete
			position:absolute
			padding-left:3px
			padding-right:3px
			top:-6px
			right:5px
			&:hover{
				transform: rotate(90deg);
				transition: transform 0.5s;
			}
			
		
		.setting
			position:absolute
			padding-left:3px
			padding-right:3px
			top:-6px
			right:30px
			&:hover
				transform: rotate(90deg);
				transition: transform 1s;
			
			
		.item
			margin:10px 0px 0px
			font-size:12px
			width:100%
			display:inline-flex
			
			.itemText
				width:245px
				white-space: nowrap
				overflow: hidden
				text-overflow: ellipsis
				&:hover
					cursor:pointer
			
			.editInput 
				width:240px
				display:inline-flex
				margin-right:5px
				
				>>>.el-input__inner
					border-radius:0px
					border-left:0px
					border-top:0px
					border-right:0px
					
			.editBtn
				font-size:12px;
				color:#009688;
				display: inline-flex;
			
		.fileSize
			text-align:right
			font-size:12px
			line-height:normal
			margin-top:-18px
			padding-right:5px
			color:#607d8b
		
		.annexImg
			text-align: center;
			line-height:normal
			
			a
				display:inline-block
				
			img
				max-width: 260px;
								
	.alertInfo
		color:red
		line-height:initial
		padding:0px
		margin:0px
		font-size:12px		

.menuItem
	padding:5px 20px
	font-size:12px	
	
	.label
		width:50px;
		display: inline-block
		padding-right:10px
		text-align: right
	